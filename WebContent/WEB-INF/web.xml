<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>HomeInsurance</display-name>
  <welcome-file-list>
    <welcome-file>welcome.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.cts.insurance.homequote.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>location</servlet-name>
    <servlet-class>com.cts.insurance.homequote.servlet.LocationServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>homeownerInfo</servlet-name>
    <servlet-class>com.cts.insurance.homequote.servlet.HomeownerInfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>property</servlet-name>
    <servlet-class>com.cts.insurance.homequote.servlet.PropertyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>quoteSummary</servlet-name>
    <servlet-class>com.cts.insurance.homequote.servlet.QuoteSummaryServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>policy</servlet-name>
    <servlet-class>com.cts.insurance.homequote.servlet.PolicyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoggerInitializer</servlet-name>
    <servlet-class>com.cts.insurance.homequote.log.LogInitializer</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>location</servlet-name>
    <url-pattern>/location</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>homeownerInfo</servlet-name>
    <url-pattern>/homeownerInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>property</servlet-name>
    <url-pattern>/property</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>quoteSummary</servlet-name>
    <url-pattern>/quoteSummary</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>policy</servlet-name>
    <url-pattern>/policy</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoggerInitializer</servlet-name>
    <url-pattern>/LoggerInitializer</url-pattern>
  </servlet-mapping>
</web-app>